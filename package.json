{
  "name": "metis",
  "version": "1.1.0",
  "description": "Metis/Gravity interacts directly with the Jupiter blockchain, encrypting and securing your information in individually permissible messaging data.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose --silent",
    "eslint": "./node_modules/.bin/esint --ext .js --ext .jsx ./ --ignore-pattern public/",
    "eslint-folders": "eslint --ext .js --ext .jsx . --ignore-pattern public/ | grep /home",
    "gulp": "gulp",
    "dev-old": "concurrently webpack --mode development && node --trace-warnings --max-old-space-size=4096 server.mjs NODE_ENV=development",
    "dev": "npx concurrently 'nodemon --trace-warnings --max-old-space-size=4096 server.mjs NODE_ENV=development' 'webpack --mode development --config ./webpack.config.cjs' ",
    "dev-orig": "webpack --mode development && node --trace-warnings server.mjs NODE_ENV=development",
    "dev-monitor": "webpack --mode development --watch && nodemon server.mjs NODE_ENV=development",
    "start": "webpack --mode production && node --max-old-space-size=4096 server.mjs NODE_ENV=production",
    "change-log": "git log --pretty=\"- %s\" > CHANGELOG.md"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ]
  },
  "authors": [
    {
      "name": "lemasters",
